<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="testsuite" verbose="1" >
    <groups>
        <run>
            <include name="regression"></include>
        </run>
    </groups>
    <listeners>
        <listener class-name="com.nopcommerce.demo.customelisteners.CustomListeners"></listener>
    </listeners>
    <test name="regression Test" >
        <classes>
            <class name="com.nopcommerce.demo.testsuit.TestSuit"></class>
            <class name="com.nopcommerce.demo.testsuit.TopMenuTest"></class>
        </classes>
    </test>
</suite>